{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "products-list",
    "pluralName": "products",
    "displayName": "Products",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "text"
    },
    "productId": {
      "type": "uid",
      "targetField": "name"
    },
    "price": {
      "type": "string"
    },
    "originalPrice": {
      "type": "string"
    },
    "imageUrl": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "tags": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::department-product.department-product",
      "inversedBy": "title"
    },
    "unitCapitity": {
      "type": "string"
    },
    "unitPrice": {
      "type": "string"
    },
    "Upc": {
      "type": "text"
    },
    "Descriptions": {
      "type": "text"
    },
    "address": {
      "type": "text"
    },
    "promotion": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::deals-product.deals-product"
    }
  }
}
